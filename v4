import React from "react";
import { Button, TextField, Typography, Grid, Box } from "@mui/material";

interface CreateEntitlementUserProps {
  onBack: () => void;
}

const CreateEntitlementUser: React.FC<CreateEntitlementUserProps> = ({ onBack }) => {
  return (
    <Box sx={{ padding: 3, backgroundColor: "#1A2028", borderRadius: 2, color: "white" }}>
      <Typography variant="h5" sx={{ marginBottom: 3 }}>
        Create Entitlement User Account
      </Typography>

      <Grid container spacing={2}>
        <Grid item xs={12} sm={6}>
          <TextField fullWidth label="Bank ID" variant="outlined" size="small" />
        </Grid>
        <Grid item xs={12} sm={6}>
          <TextField fullWidth label="Account Owner" variant="outlined" size="small" />
        </Grid>
        <Grid item xs={12} sm={6}>
          <TextField fullWidth label="Roles" variant="outlined" size="small" />
        </Grid>
        <Grid item xs={12} sm={6}>
          <TextField fullWidth label="Account Type" variant="outlined" size="small" />
        </Grid>
        <Grid item xs={12} sm={6}>
          <TextField fullWidth label="Status" variant="outlined" size="small" />
        </Grid>
      </Grid>

      <Box sx={{ marginTop: 3, display: "flex", gap: 2 }}>
        <Button variant="contained" color="primary">
          Create
        </Button>
        <Button variant="outlined" color="inherit" onClick={onBack}>
          Cancel
        </Button>
      </Box>
    </Box>
  );
};

export default CreateEntitlementUser;




{activeTab === "create-account" && (
  <CreateEntitlementUser onBack={() => setActiveTab("entitlement-users")} />
)}




<Button
  variant="contained"
  startIcon={<AddCircleIcon />}
  onClick={() =>
    onNavigate?.(activeTab === "1" ? "create-account" : "create-entitlement")
  }
>
  {activeTab === "1" ? "Create Account" : "Create Entitlement"}
</Button>
